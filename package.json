{
  "name": "slowco",
  "version": "0.0.0",
  "private": true,
  "license": "MIT",
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "dev": "turbo run dev --parallel",
    "storybook": "turbo run storybook --parallel",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{js,ts,tsx}\""
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "5.27.1",
    "@typescript-eslint/parser": "5.27.1",
    "eslint": "8.17.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-solid": "0.6.1",
    "fp-ts": "2.12.1",
    "io-ts": "2.2.16",
    "prettier": "2.6.2",
    "turbo": "1.2.16",
    "typescript": "4.7.3"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "packageManager": "yarn@1.22.18"
}
